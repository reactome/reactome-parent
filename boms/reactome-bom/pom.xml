    <reactome.fireworks-exporter.version>1.2.8</reactome.fireworks-exporter.version>
    <reactome.reaction-exporter.version>1.3.0</reactome.reaction-exporter.version>
    <reactome.search-core.version>1.5.0</reactome.search-core.version>
