      </dependency>
      <dependency>
        <groupId>org.reactome.server.tools</groupId>
        <artifactId>sbml-exporter</artifactId>
        <version>${reactome.sbml-exporter.version}</version>
      </dependency>
      <dependency>
        <groupId>org.reactome.server.tools</groupId>
